{
  "name": "dota-backend",
  "version": "1.0.0",
  "description": "Dota heroes and items as JSON data",
  "main": "server.js",
  "author": "kristi.stanescu@gmail.com",
  "license": "Apache-2.0",
  "scripts": {
    "docker-build": "npm run docker-ignore && docker build -t pocket-dota .",
    "docker-start": "docker run -t -i -e --rm -p 8080:8080 --name pocket-dota-container pocket-dota",
    "docker-clean": "docker system prune",
    "docker-explore": "docker run --rm -it --entrypoint=/bin/bash pocket-dota",
    "docker-ignore": "node node_modules/dockerignore/cli.js --unignore \"secrets/,.dockerignore,.gitignore\" --ignore \".git,api-test/\"",
    
    "debug-skip": "SKIP=heroes,items,tips npm start",
    "test-api-populate": "node api-test/testAPI",
    "test-api": "TEST_API=true npm start",
    "start-prod": "NODE_ENV=production node server",
    "start": "NODE_ENV=development node server"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^0.9.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.0",
    "dockerignore": "^0.2.0",
    "express": "^4.15.4",
    "firebase-admin": "^4.0.3",
    "isomorphic-fetch": "^2.2.1",
    "node-schedule": "^1.2.5",
    "rimraf": "^2.6.2",
    "simple-git": "^1.96.0",
    "simple-vdf": "^1.1.1",
    "util-promisifyall": "^1.0.2",
    "uuid": "^3.1.0",
    "winston": "^2.3.1",
    "winston-daily-rotate-file": "^1.7.2"
  }
}
